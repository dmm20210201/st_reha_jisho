<h3 class="text-center">通知</h3>
<hr>
<% if @notifications.exists? %>
  <div class="users-index">
    <% @notifications.each do |n| %>
    visiter:<%= n.visiter_id %>
    visited<%= n.visited_id %>
    comment:<%= n.rehabilitation_comment_id %>
    checked:<%= n.checked %><br>
    <% end %>
  </div>
  <% else %>
  <p>通知はありません</p>
<% end %>